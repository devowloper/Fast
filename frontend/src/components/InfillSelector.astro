---
import { createEventDispatcher } from 'svelte';

const dispatch = createEventDispatcher();
let infillPercentage = 20;

function handleInfillChange(event) {
  infillPercentage = event.target.value;
  dispatch('infillChange', { infillPercentage });
}
---
<select id="infill-selector" bind:value={infillPercentage} on:change={handleInfillChange}>
  <option value="10">10%</option>
  <option value="20">20%</option>
  <option value="30">30%</option>
  <option value="40">40%</option>
  <option value="50">50%</option>
  <option value="60">60%</option>
  <option value="70">70%</option>
  <option value="80">80%</option>
  <option value="90">90%</option>
  <option value="100">100%</option>
</select>
